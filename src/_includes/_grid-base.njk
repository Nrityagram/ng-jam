  <!-- carousels is only for the venue logo ticker tape on homepage -->
    {% if carousels %}
      <script src="https://unpkg.com/flickity@2/dist/flickity.pkgd.min.js"></script>
      <script>
        var flktyWrapAroundAuto = document.getElementsByClassName("flick-carousel--wraparound-autoplay")[0];
        if (flktyWrapAroundAuto) {
          var flkty_wrap = new Flickity(flktyWrapAroundAuto, {
            wrapAround: true,
            autoPlay: 2500,
            freeScroll: true,
            prevNextButtons: false,
            pageDots: false
          });
          // This avoids FOUC -- flash of unstyled content
          flktyWrapAroundAuto.style.opacity = 1.0;
          flkty_wrap.on('pointerUp', function (event, pointer) {
            flkty_wrap.playPlayer();
          });
        }

        var review_content_flkty = document.getElementById("review-content-flkty");
        var review_img_flkty = document.getElementById("review-img-flkty");
        // var review_nav_buttons = document.getElementById("review-nav-buttons");
        var view_all_button = document.getElementById("view-all-button");

        if (review_content_flkty && review_img_flkty) {
          var flkty__review_img = new Flickity('.review-img--carousel', {
            wrapAround: true,
            prevNextButtons: false,
            pageDots: false,
            draggable: false,
            asNavFor: '.review-content--carousel'
          });
          var flkty__review_content = new Flickity('.review-content--carousel', {
            wrapAround: true,
            prevNextButtons: true,
            pageDots: true,
            draggable: true
          });
          // This avoids FOUC -- flash of unstyled content
          // Turn on opacity after intialising flickity sliders
          review_content_flkty.style.opacity = 1.0;
          review_img_flkty.style.opacity = 1.0;
          // review_nav_buttons.style.opacity = 1.0;
          if (view_all_button) 
            view_all_button.style.opacity = 1.0;
          
          var previousButton = document.querySelector('.button--previous');
          if (previousButton) {
            previousButton.addEventListener('click', function () {
              flkty__review_content.previous();
              flkty__review_img.previous();
            });
          }

          var nextButton = document.querySelector('.button--next');
          if (nextButton) {
            nextButton.addEventListener('click', function () {
              flkty__review_content.next();
              flkty__review_img.next();
            });
          }
        }
      </script>
    {% endif %}

